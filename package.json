{
  "name": "asgard-frontend-template",
  "type": "module",
  "version": "1.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:main": "vite --mode main",
    "dev:micro": "vite --mode micro",
    "dev:standalone": "vite --mode development",
    "dev:test": "vite --mode test",
    "build": "vite build",
    "build:main": "vite build --mode main",
    "build:micro": "vite build --mode micro",
    "build:test": "vite build --mode test",
    "preview": "vite preview",
    "preview:test": "vite preview --mode test",
    "lint": "eslint . --ext .js,.vue,.json",
    "lint:fix": "pnpm lint --fix",
    "dep:check": "pnpm dlx npm-check-updates",
    "dep:update": "pnpm dlx npm-check-updates -u && pnpm install",
    "prepare": "husky"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.3",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "qiankun": "^2.10.16",
    "vue": "^3.5.27",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "modern-normalize": "^3.0.1",
    "sass": "^1.97.3",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "unplugin-vue-router": "^0.19.2",
    "vite": "^7.3.1"
  },
  "lint-staged": {
    "*.{vue,js,json,md,yml}": "eslint --fix"
  }
}
